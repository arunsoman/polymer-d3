{
    "env": {
        "browser": true
    },
    "extends": "google",
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": 0,
        "new-cap": 0
    },
    "globals": {
        "Polymer": true
    }
}
